config BOARD_${id.upper()}_LEFT
    bool "${name} left"
    depends on SOC_SERIES_YOUR_SOC_SERIES_HERE
    select SOC_PART_NUMBER_ABCDEFGH

config BOARD_${id.upper()}_RIGHT
    bool "${name} right"
    depends on SOC_SERIES_YOUR_SOC_SERIES_HERE
    select SOC_PART_NUMBER_ABCDEFGH
